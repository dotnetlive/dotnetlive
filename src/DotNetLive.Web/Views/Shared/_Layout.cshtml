    <script type="text/javascript" src="/lib/bootstrap/dist/js/bootstrap.js" asp-append-version="true"></script>-->
    <!-- JS Implementing Plugins -->
    <script type="text/javascript" src="//static.dotnet.live/assets/plugins/back-to-top.js" asp-append-version="true"></script>
    <script type="text/javascript" src="//static.dotnet.live/assets/plugins/smoothScroll.js" asp-append-version="true"></script>
    <script type="text/javascript" src="//static.dotnet.live/assets/plugins/modernizr.js" asp-append-version="true"></script>
    <script type="text/javascript" src="//static.dotnet.live/assets/plugins/login-signup-modal-window/js/main.js" asp-append-version="true"></script>
    <!-- Gem jQuery -->
    <script type="text/javascript" src="//static.dotnet.live/assets/plugins/jquery.parallax.js" asp-append-version="true"></script>
    <script type="text/javascript" src="//static.dotnet.live/assets/plugins/fancybox/source/jquery.fancybox.pack.js" asp-append-version="true"></script>
